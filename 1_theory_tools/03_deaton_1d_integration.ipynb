    "# Quadrature rule for E[f(y)], where x~N(Œº,œÉ)\n",
    "def quad_herm_norm(f, Œº=0, œÉ=1, n=10): \n",
    "    x, w = np.polynomial.hermite.hermgauss(n)\n",
    "    y=Œº+np.sqrt(2)*œÉ*x\n",
    "    Q=np.sum(f(y)*w)/np.sqrt(np.pi)\n",
    "    return Q"
